{
  "name": "vue-dev-base",
  "version": "1.0.0",
  "description": "Vue自用脚手架",
  "scripts": {
    "dev": "npm run sprites && cross-env NODE_ENV=dev node --max_old_space_size=2048 ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "build": "npm run imagecache && npm run imagemin && npm run sprites && cross-env NODE_ENV=production node --max_old_space_size=2048 ./node_modules/webpack/bin/webpack.js --color",
    "watch": "npm run sprites && cross-env NODE_ENV=watch node --max_old_space_size=2048 ./node_modules/webpack/bin/webpack.js --color -w",
    "sprites": "gulp spritesmith",
    "imagemin": "gulp imagemin",
    "imagecache": "gulp cleanimagecache",
    "d": "npm run dev",
    "b": "npm run build",
    "w": "npm run watch",
    "s": "npm run sprites",
    "m": "npm run imagemin",
    "c": "npm run imagecache"
  },
  "author": "benny zheng",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.0",
    "babel-polyfill": "^6.23.0",
    "mockjs": "^1.0.1-beta3",
    "vue": "^2.2.6",
    "vue-router": "^2.3.1",
    "vuex": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babili-webpack-plugin": "^0.0.11",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-cache": "^0.4.6",
    "gulp-imagemin": "^3.2.0",
    "gulp.spritesmith": "^6.4.0",
    "html-webpack-plugin": "^2.28.0",
    "imagemin-pngquant": "^5.0.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "vinyl-buffer": "^1.0.0",
    "vue-loader": "^11.3.4",
    "vue-template-compiler": "^2.2.6",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2",
    "cross-env": "^5.0.0"
  }
}
